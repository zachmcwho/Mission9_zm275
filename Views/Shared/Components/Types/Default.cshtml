@{
}

@model IEnumerable<string>

<a asp-controller="Home" asp-action="Index" asp-route-projectType ="" asp-route-pageNum ="1" class="btn btn-outline-secondary">Home</a>

@foreach (string t in Model)
            {
                <a asp-controller="Home" asp-action="Index" asp-route-projectType ="@t" asp-route-pageNum ="1"
                   class="btn @(ViewBag.SelectedTypes == t ? "btn-Primary" :  "btn-outline-secondary")">@t</a>
            }